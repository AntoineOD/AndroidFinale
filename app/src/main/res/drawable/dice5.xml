<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Background rounded rectangle -->
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <solid android:color="#FFFFFF" />
        </shape>
    </item>

    <!-- Top left dot -->
    <item android:id="@+id/hautGauche" android:top="4dp" android:left="4dp">
        <shape android:shape="oval">
            <size android:width="2dp" android:height="2dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>

    <!-- Top right dot -->
    <item android:id="@+id/hautDroit" android:top="4dp" android:right="4dp">
        <shape android:shape="oval">
            <size android:width="2dp" android:height="2dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>

    <!-- Bottom left dot -->
    <item android:id="@+id/basGauche" android:bottom="4dp" android:left="4dp">
        <shape android:shape="oval">
            <size android:width="2dp" android:height="2dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>

    <!-- Bottom right dot -->
    <item android:id="@+id/basDroit" android:bottom="4dp" android:right="4dp">
        <shape android:shape="oval">
            <size android:width="2dp" android:height="2dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>

    <!-- Center dot -->
    <item android:id="@+id/centre" android:gravity="center">
        <shape android:shape="oval">
            <size android:width="2dp" android:height="2dp"/>
            <solid android:color="#000000" />
        </shape>
    </item>
</layer-list>